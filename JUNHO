                    char monster_information[70] = "Captain Ddong";
                }
                else if(monster_number == 2)
                {
                    char monster_information[70] = "Scarlet Onsi";
                }
                else if(monster_number == 3)
                {
                    char monster_information[70] = "Iron Boy";
                }
                else
                {
                    char monster_information[70] = "Juno Panther";
                }
                printf("%dыНШьаД) %sъ░А ыВШэГАыВмьК╡ыЛИыЛд.\n", event+1, monster_information);

                char monster_week[20];
                monster_WN = (rand() % 4) +1;
                if(monster_WN == 1)
                {
                    char monster_week[20] = "ьШдые╕ьЖР";
                }
                else if(monster_WN == 2)
                {
                    char monster_week[20] = "ьЩ╝ьЖР";
                }
                else if(monster_WN == 3)
                {
                    char monster_week[20] = "ьШдые╕ы░Ь";
                }
                else
                {
                    char monster_week[20] = "ьЩ╝ы░Ь";
                }
                printf("       (Tip: %sьЭ┤ ьХ╜ьаРьЮЕыЛИыЛд.)\n", monster_week);

                    if (event_Exp >= 0)
                    {
                        printf("       %sые╝ ь▓Шь╣ШэХШьШАьК╡ыЛИыЛд.\n", monster_information);
                        printf("       ЁЯТЦ   %.2fьЭШ ъ▓╜эЧШь╣Шые╝ эЪНыУЭэЦИьК╡ыЛИыЛд.\n\n", event_Exp);
                    }
                    else
                    {
                        printf("       %sые╝ ь▓Шь╣ШэХШьзА ык╗ эЦИьК╡ыЛИыЛд.\n", monster_information);
                        printf("       ЁЯТФ   %.2fьЭШ ъ▓╜эЧШь╣Шъ░А ь░иъ░РыРШьЧИьК╡ыЛИыЛд.\n\n", event_Exp);
                    }
                if(event == 4) // ыЛиъ│Д ыВ┤ ьЭ┤ы▓дэК╕ (6ы▓Иьз╕-ы│┤ьКд)
                {
                    printf("ы│┤ьКд ыкмьКдэД░ тЪбтЪбтЪб   ып╝ъ▒┤ ьКдэК╕ыаИьЭ╕ьзА    тЪбтЪбтЪбъ░А ыУ▒ьЮе эЦИьК╡ыЛИыЛд.\n");
                    Exp = Exp * Boss_rand;                  // ъ╕░ьб┤ expьЧР ы│┤ьКд ь╢Фъ░Аъ▓╜эЧШь╣Шъ│▒ 1.0~5.0ыЮЬыНдэХЬ ъ░Т ъ│▒
                }
            }

            while(Exp > Exp_box)                             // ыаИы▓иьЧЕ!
            {
                printf("ь╢ХэХШэХйыЛИыЛд ыаИы▓иьЧЕ эХШьЕиьК╡ыЛИыЛд!\n");
                printf("#$%%^^*ьзАъ╕ИыЛ╣ьЮе ьД▒ьЮе эМиэВдьзА ъ╡мызд!!!@#$^*\n");
                level++;

                Exp = Exp - Exp_box;                    // ь┤Иъ│╝ ъ▓╜эЧШь╣Ш ьЭ┤ьЫФ
                Exp_box = Exp_box * Exp_boxrand;
                printf("ыаИы▓и ьЧЕ!\nэШДьЮм %dыаИы▓иьЮЕыЛИыЛд.\n", level); 
                printf("%dыаИы▓иьЧР эХДьЪФэХЬ ъ▓╜эЧШь╣ШыКФ %.2fьЮЕыЛИыЛд.\n", level+1 ,Exp_box);

                if(level >= 5)                                  // 99ыаЩьЭ┤ьГБьЭ┤ый┤ ьвЕыгМ
                {
                    break;
                }
            }
            
            while(Exp < Exp_box && Exp < 0)                         // ъ▓╜эЧШь╣Шъ░А ьЪФъ╡мыЯЙы│┤ыЛд ьЮСъ▒░ыВШ ьЭМьИШьЭ┤ый┤
            {
                printf("ьЭ┤ыЯ░ ыаИы▓иьЭ┤ эХШыЭ╜эЦИьК╡ыЛИыЛд. ьб░ьЛмэХШьЛньЛЬьЪФ ы│┤ьКдыКФ ъ░ХэХйыЛИыЛд.\n");
                printf("эМБ : IRON ыкмьКдэД░ыКФ ьШдые╕ьЖРьЭ┤ ьХ╜ьаРьЮЕыЛИыЛд.\n");
                level--;
                Exp = Exp_box - Exp;                                
                Exp_box = Exp_box / Exp_boxrand;                    // ъ▓╜эЧШь╣Ш ьЪФъ╡мыЯЙ ъ░РьЖМ
                printf("ыаИы▓и ыЛдьЪ┤!\nэШДьЮм %dыаИы▓иьЮЕыЛИыЛд.\n", level);
            }
            turn++;                                          // эД┤ ьИШ ьжЭъ░А
            printf("\n");

            printf("эШДьЮм ъ▓╜эЧШь╣ШыКФ %.2f ьЮЕыЛИыЛд.\n",Exp);
            printf("%dыаИы▓иьЮЕыЛИыЛд.\n", level);
            printf("ьзАъ╕Иъ╣МьзА %dэД┤ьЭ┤ ьзДэЦЙыРмьК╡ыЛИыЛд.\n", turn);
            printf("ьзДэЦЙьдСьЭ╕ ьКдэЕМьЭ┤ьзАыКФ %dьЮЕыЛИыЛд.", stage);
            printf("ыЛдьЭМ ыкиэЧШьЭД ьзДэЦЙэХШыадый┤ 1ьЭД ьЮЕыае\nый┤эБ┤ыжмьЦ┤эХЬ ьКдэЕМьЭ┤ьзАыбЬ ыПМьХДъ░ДыЛдый┤ 2ые╝ ьЮЕыае\nэБ┤ыжмьЦ┤эХЬ ьКдэЕМьЭ┤ьзАьЭШ ьЭ┤ьаД ьКдэЕМьЭ┤ьзАыбЬ ыПМьХДъ░Аъ╕╕ ьЫРэХЬыЛдый┤ 3ьЭД ьЮЕыае:\n");
            scanf("%d", &user);
            
            if (user == 1)
            {
                printf("ыЛдьЭМ ыкиэЧШьЭД ьзДэЦЙэХйыЛИыЛд.\n");
                if(level == 5)                                  // 99ыаЩьЭ┤ьГБьЭ┤ый┤ ьвЕыгМ
                {
                    break;
                }
                else if(level < 1)                  // 1ыаЩьЭ┤ый┤ьДЬ ъ▓╜эЧШь╣Шъ░А ьЭМьИШый┤ эМиы░░
                {
                    break;
                }
                continue;
                        
            }
            else if(user == 2)
            {
                printf("эХ┤ыЛ╣ эЪМь░иые╝ ыЛдьЛЬ ыПДьаДэХйыЛИыЛд.\n");
                --stage;
                if(level == 5)                                  // 99ыаЩьЭ┤ьГБьЭ┤ый┤ ьвЕыгМ
                {
                    break;
                }
                else if(level < 1)                  // 1ыаЩьЭ┤ый┤ьДЬ ъ▓╜эЧШь╣Шъ░А ьЭМьИШый┤ эМиы░░
                {
                    break;
                }
                    continue;
            }
            else
            {
                printf("ьЭ┤ьаД эЪМь░иые╝ ыПДьаДэХйыЛИыЛд.\n");
                --stage;
                --stage;
                if(stage <=-1)              // stageъ░А ьЭМьИШыбЬ ыРШыКФ ъ▓ГьЭД ызЙъ╕░ ьЬДэХЬ ьЮеь╣Ш
                {
                    stage = 0;
                }
                if(level == 5)                                  // 99ыаЩьЭ┤ьГБьЭ┤ый┤ ьвЕыгМ
                {
                    break;
                }
                else if(level < 1)                  // 1ыаЩьЭ┤ый┤ьДЬ ъ▓╜эЧШь╣Шъ░А ьЭМьИШый┤ эМиы░░
                {
                    break;
                }
                continue;
            }
            printf("\n");
        }
        printf("\n");

        if(level >= 5)                                  // 99ыаЩьЭ┤ьГБьЭ┤ый┤ ьвЕыгМ
        {
            printf("ьаДьДдьЭШ ь╢ЬэШД! ъ░Ьы░ЬьЫРьЧРьДЬ эГИь╢ЬэХШьЕиьК╡ыЛИыЛд.!\n");

            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтбатгатввтб▓тб░тбвтбатгАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаА\n");
