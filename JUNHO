            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаатбСтвМтавтгСтгмтготг┐тб┐тг┐тбЗтбгтбКтбвтв▒та╕тбИтаРтаИтаДтвдтблтгЮтвЦтв╜тб╣тбктвптб║тгЪтботб╖тгЭтгптб┐тг║твптгЯтгЧтб┐тб▒тбХтбЬтвМтавтаКтаДтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтаАтвАтаФтбСтгЬтг░тгптг┐тв┐тг╗тг╖тг┐тб┐тг┐тбФтбМтвМтаЖтбХтбРтаатаБтгВтв╝тб║твктвНта╢тбХтгЭтгЮтботбПтбЮтгЭтв╜тботгЪтвотг│тб╗тг╝тб│тб▒тбХтбХтбЕтвХтабтабтабтаАтаАтаАтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтаАтаАтаАтаАтаАтбатгСтг╝тг║тгЯтгптг╖тг┐тг┐тв┐тгптг╖тг┐тг┐тг┐тгОтвЖтагтвХтвФтгРтветвЬтб╡тгГтг│твзтвУтаЬтбЬтг╛твЬтвотбгтбгтглтв┐тб╕тгктв╛тгЭтб╖тб╣тбЬтбОтбОтбОтбвтбСтаЕтаЕтаЕтаДтаАтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтаАтаАтвАтг┤тв╛тг┐тг╗тгЯтгптг┐тг┐тг╗тг╜тг╛тг┐тгптг┐тг╜тб╛тгптб┐тгЯтгптвптгЧтв╖тв╜твХтбптбвтгЯтгОтвзтвСтвЭтвЮтботггтвЧтгЭтв╝тв╜тбптг║тг│твптв║твЬтвотвктвзтвгтаХтбЬтвМтактбИтбВтаДтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтаАтвАтгЬтв╜тгЯтг┐тгптг┐тгЯтг┐тгптг┐тб┐тгЯтг┐тг╜тг┐тг╜тгптг┐тгптв┐тг╗тгЮтгптв╛тв╜тв╜твХтбптгЗтг╗тг║тгктгГтвктв│тглтв╛тгЭтгЮтготг╗тг╜тг╗твотв│твХтвзтв│таЙтвЧтвзтбгтбгтбСтбМтаФтбатвБтаАтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаАтгатб│тгХтвптг┐тгптг╖тг┐тгЯтгптг┐тгптг┐тг┐тб┐тб┐тг╜тг┐тг╖тб┐тг╛тв┐тг╜тв╛тг╜тб╜тг╜тгЭтгЧтвптгЮтвЬтг╖тг│тг│тгХтвЭтв╛тв╜тг╛тв╛тб╛тгптвЯтботботб│тгЭтв╝тб▒таАтаИта│тгЭтвЬтвМтввтвСтаФтбБтаДтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтв░тб▒тгЭтв╛тв╜тг║тг┐тг╜тг╛тг┐тг╗тг┐тг╜тг┐тб╖тб╗тб╜тг╜тг┐тг┐тг╗тгЯтг┐тг╜тгЯтг╖тг╗тгЧтб╖тбптб╖тб╜тб╡тг╜тгЮтв╖тв╜тв╜тг│тгЧтб┐тбптвптблтгЧтвптб║тбЭтгОтвотбВтаАтаАтаАтаИтвзтвгтагтбВтбГтбВтбВтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтб│тгЭтв╛твЭтгЧтв╖тв╜твптг╖тг┐тв┐тгптг┐тг│тблтгЭтвЮтвотг┐тг┐тг┐тг╜тв┐тгЮтг┐тгЮтг╖твптб┐тб╜тгптвптгЧтгЧтв╛тв╜твЭтгЭтвотб║тг║тгктб│тгЭтвЬтвотв║тбктб║тбЬтбЖтаАтаАтаАтаАтб░тбСтвМтвВтвВтавтаРтаАтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтвЭтгЧтв╜тб│тб│тгЭтв╡тблтаЛтаЙтаЙтбптгктвЮтвотб│тгЭтв╡тб┐тг┐тг┐тг╛тв┐тг╗тб╖тг┐тв╜тбптг┐тв╜тбптгЯтб╛тг╜тв╜тв╜тгЭтвотб│тгЭтвотгЦтв╜твЬтбЭтгОтвзтблтб║тгЬтаВтаАтаАтаАтаАтвитгктгжтгзтб╢тгмтгМтбДтаАтаАтаАтаАтаА\n");
            printf("таАтаАтаАтаИтаШтаСта╜та╣таЬтаБтаАтаАтаАтаАтбптботглтвЧтгЭтвотв│тг╗тг┐тг┐тг┐тг╗тгЯтг┐тг╗тгптб┐тг╜твптв┐тв╜тг╜тб│тбптб╖тб╜тгХтвптб║тгХтв╖тв╜та╡тгЭтвОтвзтблтг║тв╕таАтаАтаАтаАтаАтв╕тг╜тг╛тв╖тг╗тб╜тг╖тбЗтаАтаАтаАтаАтаА\n");
        }
        else if(level < 1)                  // 1ыаЩьЭ┤ый┤ьДЬ ъ▓╜эЧШь╣Шъ░А ьЭМьИШый┤ эМиы░░
        {
            printf("YOU DIE.\n");
            printf("ыЛ╣ьЛаьЭШ ьЛдыаеьЬ╝ыба ьХДьзБ ы╢Аьб▒эХйыЛИыЛд 6ъ░ЬьЫФ ыНФ ъ│╡ы╢АэХШьД╕ьЪФ.\n");
        }
        if(level >= 5 || level < 1)
        {
            int Replay;
            printf("ъ▓МьЮДьЭД ыЛдьЛЬ ьзДэЦЙэХШъ│а ьЛ╢ьЬ╝ый┤ 7, ъ▓МьЮДьЭД ьвЕыгМэХЬыЛдый┤ 7ьЭД ьаЬьЩ╕эХЬ ьИльЮРые╝ ыИМыЯмьг╝ьЛньЛЬьШд.\n");
            scanf("%d", &Replay);
    
            if (Replay == 7)
            {
                continue;
            }
            else
            {
                break;
            }

        }
    }
    // printf("%d\n", Exp);
    printf("ьвЕыгМ!");

    
    return 0;
}
